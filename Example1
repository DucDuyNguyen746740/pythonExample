x = int(input("Enter your guess 1: "))
y = int(input("Enter your guess 2: "))
z = int(input("Enter your guess 3: "))

if x <= y <= z:
    print "number max: " + str(z)
elif x <= y and y >= z:
    print "number max: " + str(y)
# elif x > y > z:
#     print "number max: "+str(x)
else:
    print "number max: " + str(x)

if x**2 == (y**2 + z**2) or y**2 == (x**2 + z**2) or z**2 == (y**2 + x**2):
    print "triangle"
else:

    print "not"
